{
    "name": "paredit-revived",
    "displayName": "Paredit Revived",
    "description": "Provides VS Code with structural editing and navigation for LISPs",
    "version": "0.1.5",
    "icon": "assets/paredit.png",
    "publisher": "cospaia",
    "license": "MIT",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages",
        "Formatters",
        "Other"
    ],
    "keywords": [
        "clojure",
        "lisp",
        "scheme",
        "brackets",
        "paredit"
    ],
    "galleryBanner": {
        "color": "#344D81",
        "theme": "dark"
    },
    "badges": [
        {
            "url": "https://img.shields.io/badge/clojurians-editors-blue.svg?logo=slack",
            "href": "https://clojurians.slack.com/messages/editors/",
            "description": "Let's talk in the #editors channel on the Clojurians Slack"
        },
        {
            "url": "https://img.shields.io/badge/vscode--dev--community-clojure-blue.svg?logo=slack",
            "href": "https://vscode-dev-community.slack.com/messages/clojure/",
            "description": "See you in the #clojure channel on the VS Code Dev Slack"
        }
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/PEZ/paredit-for-vscode"
    },
    "activationEvents": [
        "onLanguage:clojure",
        "onCommand:paredit.forwardSexp",
        "onCommand:paredit.backwardSexp",
        "onCommand:paredit.forwardDownSexp",
        "onCommand:paredit.backwardUpSexp",
        "onCommand:paredit.sexpRangeExpansion",
        "onCommand:paredit.sexpRangeContracrion",
        "onCommand:paredit.closeList",
        "onCommand:paredit.rangeForDefun",
        "onCommand:paredit.slurpSexpForward",
        "onCommand:paredit.slurpSexpBackward",
        "onCommand:paredit.barfSexpForward",
        "onCommand:paredit.barfSexpBackward",
        "onCommand:paredit.spliceSexp",
        "onCommand:paredit.splitSexp",
        "onCommand:paredit.killSexpForward",
        "onCommand:paredit.killSexpBackward",
        "onCommand:paredit.spliceSexpKillForward",
        "onCommand:paredit.spliceSexpKillBackward",
        "onCommand:paredit.wrapAroundParens",
        "onCommand:paredit.wrapAroundSquare",
        "onCommand:paredit.wrapAroundCurly",
        "onCommand:paredit.indentRange",
        "onCommand:paredit.transpose"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "category": "Paredit",
                "command": "paredit.toggle",
                "title": "Toggle"
            },
            {
                "category": "Paredit",
                "command": "paredit.forwardSexp",
                "title": "Forward Sexp"
            },
            {
                "category": "Paredit",
                "command": "paredit.backwardSexp",
                "title": "Backward Sexp"
            },
            {
                "category": "Paredit",
                "command": "paredit.forwardDownSexp",
                "title": "Down Sexp"
            },
            {
                "category": "Paredit",
                "command": "paredit.backwardUpSexp",
                "title": "Up Sexp"
            },
            {
                "category": "Paredit",
                "command": "paredit.sexpRangeExpansion",
                "title": "Sexp Range Expansion"
            },
            {
                "category": "Paredit",
                "command": "paredit.sexpRangeContraction",
                "title": "Sexp Range Contraction (Shrink Selection)"
            },
            {
                "category": "Paredit",
                "command": "paredit.closeList",
                "title": "Close List"
            },
            {
                "category": "Paredit",
                "command": "paredit.rangeForDefun",
                "title": "Range For Defn"
            },
            {
                "category": "Paredit",
                "command": "paredit.slurpSexpForward",
                "title": "Slurp Sexp Forward"
            },
            {
                "category": "Paredit",
                "command": "paredit.slurpSexpBackward",
                "title": "Slurp Sexp Backward"
            },
            {
                "category": "Paredit",
                "command": "paredit.barfSexpForward",
                "title": "Barf Sexp"
            },
            {
                "category": "Paredit",
                "command": "paredit.barfSexpBackward",
                "title": "Barf Sexp"
            },
            {
                "category": "Paredit",
                "command": "paredit.spliceSexp",
                "title": "Splice Sexp"
            },
            {
                "category": "Paredit",
                "command": "paredit.splitSexp",
                "title": "Split Sexp"
            },
            {
                "category": "Paredit",
                "command": "paredit.killSexpForward",
                "title": "Kill Sexp Forward"
            },
            {
                "category": "Paredit",
                "command": "paredit.killSexpBackward",
                "title": "Kill Sexp Backward"
            },
            {
                "category": "Paredit",
                "command": "paredit.spliceSexpKillForward",
                "title": "Splice & Kill Forward"
            },
            {
                "category": "Paredit",
                "command": "paredit.spliceSexpKillBackward",
                "title": "Splice & Kill Backward"
            },
            {
                "category": "Paredit",
                "command": "paredit.wrapAroundParens",
                "title": "Wrap Around ()"
            },
            {
                "category": "Paredit",
                "command": "paredit.wrapAroundSquare",
                "title": "Wrap Around []"
            },
            {
                "category": "Paredit",
                "command": "paredit.wrapAroundCurly",
                "title": "Wrap Around {}"
            },
            {
                "category": "Paredit",
                "command": "paredit.indentRange",
                "title": "Indent"
            },
            {
                "category": "Paredit",
                "command": "paredit.transpose",
                "title": "Transpose"
            }
        ],
        "keybindings": [
            {
                "command": "paredit.forwardSexp",
                "key": "ctrl+right",
                "mac": "ctrl+right",
                "when": "editorTextFocus"
            },
            {
                "command": "paredit.backwardSexp",
                "key": "ctrl+left",
                "mac": "ctrl+left",
                "when": "editorTextFocus"
            },
            {
                "command": "paredit.forwardDownSexp",
                "key": "ctrl+down",
                "mac": "ctrl+down",
                "when": "editorTextFocus"
            },
            {
                "command": "paredit.backwardUpSexp",
                "key": "ctrl+up",
                "mac": "ctrl+up",
                "when": "editorTextFocus"
            },
            {
                "command": "paredit.sexpRangeExpansion",
                "key": "ctrl+w",
                "mac": "ctrl+w",
                "when": "editorTextFocus"
            },
            {
                "command": "paredit.sexpRangeContraction",
                "key": "ctrl+shift+w",
                "when": "editorTextFocus"
            },
            {
                "command": "paredit.slurpSexpForward",
                "key": "ctrl+alt+.",
                "mac": "ctrl+alt+.",
                "when": "editorTextFocus"
            },
            {
                "command": "paredit.slurpSexpBackward",
                "key": "ctrl+alt+shift+.",
                "mac": "ctrl+alt+shift+.",
                "when": "editorTextFocus"
            },
            {
                "command": "paredit.barfSexpForward",
                "key": "ctrl+alt+,",
                "mac": "ctrl+alt+,",
                "when": "editorTextFocus"
            },
            {
                "command": "paredit.barfSexpBackward",
                "key": "ctrl+alt+shift+,",
                "mac": "ctrl+alt+shift+,",
                "when": "editorTextFocus"
            },
            {
                "command": "paredit.spliceSexp",
                "key": "ctrl+alt+s",
                "mac": "ctrl+alt+s",
                "when": "editorTextFocus"
            },
            {
                "command": "paredit.splitSexp",
                "key": "ctrl+alt+/",
                "mac": "ctrl+alt+/",
                "when": "editorTextFocus"
            },
            {
                "command": "paredit.killSexpForward",
                "key": "cmd+shift+backspace",
                "mac": "cmd+shift+backspace",
                "when": "editorTextFocus"
            },
            {
                "command": "paredit.killSexpBackward",
                "key": "cmd+backspace",
                "mac": "cmd+backspace",
                "when": "editorTextFocus"
            },
            {
                "command": "paredit.spliceSexpKillForward",
                "key": "ctrl+alt+up",
                "mac": "ctrl+alt+up",
                "when": "editorTextFocus"
            },
            {
                "command": "paredit.spliceSexpKillBackward",
                "key": "ctrl+alt+down",
                "mac": "ctrl+alt+down",
                "when": "editorTextFocus"
            },
            {
                "command": "paredit.wrapAroundParens",
                "key": "ctrl+alt+shift+9",
                "mac": "ctrl+alt+shift+9",
                "when": "editorTextFocus"
            },
            {
                "command": "paredit.wrapAroundSquare",
                "key": "ctrl+alt+[",
                "mac": "ctrl+alt+[",
                "when": "editorTextFocus"
            },
            {
                "command": "paredit.wrapAroundCurly",
                "key": "ctrl+alt+shift+[",
                "mac": "ctrl+alt+shift+[",
                "when": "editorTextFocus"
            },
            {
                "command": "paredit.indentRange",
                "key": "ctrl+alt+i",
                "mac": "ctrl+alt+i",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "dependencies": {
        "paredit.js": "^0.2.13"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}